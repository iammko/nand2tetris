/**
 * 8-way demultiplexor:
 * {a, b, c, d, e, f, g, h} = {in, 0, 0, 0, 0, 0, 0, 0} if sel == 000
 *                            {0, in, 0, 0, 0, 0, 0, 0} if sel == 001
 *                            etc.
 *                            {0, 0, 0, 0, 0, 0, 0, in} if sel == 111
 */

CHIP DMux8Way {
    IN in, sel[3];
    OUT a, b, c, d, e, f, g, h;

    PARTS:
    // Put your code here:
    DMux(in=in, sel=sel[2], a=sabcd, b=sefgh);

    DMux(in=sabcd, sel=sel[1], a=sab, b=scd);
    DMux(in=sefgh, sel=sel[1], a=sef, b=sgh);

    DMux(in=sab, sel=sel[0], a=a, b=b);
    DMux(in=scd, sel=sel[0], a=c, b=d);
    DMux(in=sef, sel=sel[0], a=e, b=f);
    DMux(in=sgh, sel=sel[0], a=g, b=h);

}